{
    "injectionSelector": "L:comment.block.documentation",
    "patterns": [
        {
            "include": "#natspecbody"
        }
    ],
    "repository": {
        "natspecbody": {
            "begin": "(?<=/\\*\\*)([^*]|\\*(?!/))*$",
            "while": "(^|\\G)\\s*\\*(?!/)(?=([^*]|[*](?!/))*$)",
            "patterns": [
                {
                    "include": "text.html.markdown#fenced_code_block_js"
                },
                {
                    "include": "text.html.markdown#fenced_code_block_solidity"
                },
                {
                    "include": "text.html.markdown#fenced_code_block_unknown"
                },
                {
                    "include": "#example"
                },
                {
                    "include": "source.ts#docblock"
                },
                {
                    "include": "text.html.markdown#inline"
                }
            ]
        },
        "example": {
            "begin": "((@)example)\\s+(?=([^*]|[*](?!/))*$).*$",
            "while": "(^|\\G)\\s(?!@)(?=([^*]|[*](?!/))*$)",
            "beginCaptures": {
                "1": {
                    "name": "storage.type.class.natspec"
                },
                "2": {
                    "name": "punctuation.definition.block.tag.natspec"
                }
            },
            "contentName": "meta.embedded.block.example.source.solidity",
            "patterns": [
                {
                    "include": "source.solidity"
                }
            ]
        }
    },
    "scopeName": "documentation.injection.natspec"
}